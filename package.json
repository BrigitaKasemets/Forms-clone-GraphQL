{
  "name": "forms-clone-graphql",
  "version": "1.0.0",
  "description": "GraphQL API for Forms Clone - a Google Forms alternative built with Node.js, Express, and GraphQL",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "init-db": "node scripts/init-db.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "echo 'No build step required for Node.js'"
